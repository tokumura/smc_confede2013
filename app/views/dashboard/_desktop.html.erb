<h1>Dashboard</h1>
<div class="span12">
  <div class="row">
    <div class="span2 well">
      <%= render :partial => "ranking" %>
    </div>
    <div class="span8 well">
      <div class="row">
        <div class="span7 well">
          <%= render :partial => "news" %>
        </div>
      </div>
      <div class="row">
        <div class="span7 well">
          <ul class="nav nav-tabs">
            <li class="active">
              <a href="#highlight" data-toggle="tab">ハイライト</a>
            </li>
            <li class="">
              <a href="#part_1" data-toggle="tab">予想パート１</a>
            </li>
            <li>
              <a href="#part_2" data-toggle="tab">予想パート２</a>
            </li>
          </ul>
          <div class="tab-content">
            <div class="tab-pane fade active in" id="highlight">
              <%= render :partial => "highlight" %>
            </div>
            <div class="tab-pane fade" id="part_1">
              <%= render :partial => "result_part_1" %>
            </div>
            <div class="tab-pane fade" id="part_2">
              <%= render :partial => "result_part_2" %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

